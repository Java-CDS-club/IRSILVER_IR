<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="User_Company_Detail_View"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  SelectList="TblUserDetail.USER_DETAIL_ID, 
       TblUserDetail.USER_M_ID, 
       TblUserDetail.COMPANY_ID, 
       TblUserDetail.COMPANY_ID AS COMPANY_ID1, 
       TblUserDetail.COMPANY_ID AS COMPANY_ID2, 
       TblCompany.NAME, 
       TblCompany.ID"
  FromList="TBL_USER_DETAIL TblUserDetail, TBL_COMPANY TblCompany"
  Where="TblUserDetail.USER_M_ID=:P_User_Id  
AND TblCompany.ID=TblUserDetail.COMPANY_ID">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.ERP.VO.User_Company_Detail_View_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_User_Id"
    Kind="where"
    Type="java.math.BigDecimal">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="User_Company_Detail_View"/>
  </Variable>
  <ViewAccessor
    Name="Company_Name_LOV1"
    ViewObjectName="model.LOV.Company_Name_LOV"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_CompanyId"
    ListVOName="Company_Name_LOV1"
    ListRangeSize="-1"
    NullValueFlag="none"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="CompanyId"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Name"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <EntityUsage
    Name="TblUserDetail"
    Entity="model.UserRole.EO.TblUserDetail"/>
  <EntityUsage
    Name="TblCompany"
    Entity="model.ERP.EO.TblCompany"
    ReadOnly="true"
    DeleteParticipant="false">
    <DesignTime>
      <Attr Name="_customjoin" Value="true"/>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
    Name="UserDetailId"
    PrecisionRule="true"
    EntityAttrName="UserDetailId"
    EntityUsage="TblUserDetail"
    AliasName="USER_DETAIL_ID"/>
  <ViewAttribute
    Name="UserMId"
    PrecisionRule="true"
    EntityAttrName="UserMId"
    EntityUsage="TblUserDetail"
    AliasName="USER_M_ID"/>
  <ViewAttribute
    Name="CompanyId"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblUserDetail"
    AliasName="COMPANY_ID"
    LOVName="LOV_CompanyId">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="combo"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CompanyId1"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblUserDetail"
    AliasName="COMPANY_ID1"/>
  <ViewAttribute
    Name="CompanyId2"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="TblUserDetail"
    AliasName="COMPANY_ID2"/>
  <ViewAttribute
    Name="CompanyName"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblCompany"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblCompany"
    AliasName="ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
